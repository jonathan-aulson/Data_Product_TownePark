	<owl:Class rdf:about="&fibo-actus-acv;ACTUSCashFlowFamily">
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isDefinedIn"/>
				<owl:hasValue rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;classifies"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-csf;CashFlowStructure"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-doc;refersTo"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&fibo-fnd-agr-ctr;WrittenContract">
							</rdf:Description>
							<rdf:Description rdf:about="&fibo-fbc-pas-caa;InvestmentOrDepositAccount">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>ACTUS cashflow family</rdfs:label>
		<skos:definition>classifier for the cashflows generated by a group of credit agreements, financial instruments, or related contracts based on their common characteristics from an ACTUS perspective</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&fibo-actus-acv;ACTUSCashFlowFamily-Basic">
		<rdf:type rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily"/>
		<rdfs:label>ACTUS cashflow family - basic</rdfs:label>
		<dct:source>https://www.actusfrf.org/taxonomy</dct:source>
		<owl:differentFrom rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-Combined"/>
		<owl:differentFrom rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-CreditEnhancement"/>
		<skos:definition>cashflow family classifier that applies to single (simple) contracts</skos:definition>
		<cmns-av:explanatoryNote>Basic, or atomic contracts include those individual contracts that have schedules associated with them, i.e., that have traditional maturity dates and future cash flows, or that represent demand deposits, i.e., accounts that have no structured maturity term, whose cash flows or amounts depend on counterparty behavior, as well as other assets such as stocks. Such contracts and related assets are typically bilateral.</cmns-av:explanatoryNote>
		<cmns-col:isMemberOf rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
		<cmns-dsg:isDefinedIn rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
		<cmns-txt:hasTextValue>Basic</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-actus-acv;ACTUSCashFlowFamily-Combined">
		<rdf:type rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily"/>
		<rdfs:label>ACTUS cashflow family - combined</rdfs:label>
		<dct:source>https://www.actusfrf.org/taxonomy</dct:source>
		<owl:differentFrom rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-Basic"/>
		<owl:differentFrom rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-CreditEnhancement"/>
		<skos:definition>cashflow family classifier that applies to combined (complex, derived) contracts, such as derivatives</skos:definition>
		<cmns-col:isMemberOf rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
		<cmns-dsg:isDefinedIn rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
		<cmns-txt:hasTextValue>Combined</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-actus-acv;ACTUSCashFlowFamily-CreditEnhancement">
		<rdf:type rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily"/>
		<rdfs:label>ACTUS cashflow family - credit enhancement</rdfs:label>
		<dct:source>https://www.actusfrf.org/taxonomy</dct:source>
		<owl:differentFrom rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-Basic"/>
		<owl:differentFrom rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-Combined"/>
		<skos:definition>cashflow family classifier that applies to agreements that are related to other agreements or accounts, and provide credit support to existing contracts</skos:definition>
		<cmns-col:isMemberOf rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
		<cmns-dsg:isDefinedIn rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
		<cmns-txt:hasTextValue>Credit Enhancement</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&fibo-actus-acv;ACTUSCashflowCategory">
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isDefinedIn"/>
				<owl:hasValue rdf:resource="&fibo-actus-acv;AlgorithmicContractTypesClassificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;classifies"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-csf;CashFlowStructure"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-doc;refersTo"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&fibo-fnd-agr-ctr;WrittenContract">
							</rdf:Description>
							<rdf:Description rdf:about="&fibo-fbc-pas-caa;InvestmentOrDepositAccount">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>ACTUS cashflow category</rdfs:label>
		<skos:definition>classifier for the cashflows generated by a group of credit agreements, financial instruments, or related contracts, within an ACTUS cashflow family, based on their more specific, common characteristics</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&fibo-actus-acv;ACTUSCashflowCategory-Asymmetric">
		<rdf:type rdf:resource="&fibo-actus-acv;ACTUSCashflowCategory"/>
		<rdfs:label>ACTUS cashflow category - asymmetric</rdfs:label>
		<dct:source>https://www.actusfrf.org/taxonomy</dct:source>
		<skos:definition>cashflow category that applies to combined contracts processed as members of the asymmetric class</skos:definition>
