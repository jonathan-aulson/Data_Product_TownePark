	<owl:NamedIndividual rdf:about="&fibo-actus-acv;ACTUSContractType-ExoticOption">
		<rdf:type rdf:resource="&fibo-actus-acv;ACTUSContractType"/>
		<rdf:type>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-doc;refersTo"/>
				<owl:someValuesFrom rdf:resource="&fibo-der-drc-opt;ExoticOption"/>
			</owl:Restriction>
		</rdf:type>
		<rdfs:label>ACTUS contract type - exotic option</rdfs:label>
		<skos:definition>cashflow classifier that applies to exotic options</skos:definition>
		<skos:isNarrowerThan rdf:resource="&fibo-actus-acv;ACTUSCashFlowFamily-Combined"/>
