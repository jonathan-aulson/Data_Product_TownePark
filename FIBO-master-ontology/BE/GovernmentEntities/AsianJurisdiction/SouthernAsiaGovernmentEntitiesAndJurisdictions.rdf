		<cmns-rga:governs rdf:resource="&lcc-3166-1;Afghanistan"/>
		<cmns-rga:hasJurisdiction rdf:resource="&fibo-be-ge-sasj;JurisdictionOfAfghanistan"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-be-ge-sasj;GovernmentOfTheIslamicRepublicOfIran">
